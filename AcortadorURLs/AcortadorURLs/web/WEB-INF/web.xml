<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorMenu</servlet-name>
        <servlet-class>Controlador.ControladorMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorSuscripcion</servlet-name>
        <servlet-class>Controlador.ControladorSuscripcion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorPerfil</servlet-name>
        <servlet-class>Controlador.ControladorPerfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorURL</servlet-name>
        <servlet-class>Controlador.ControladorURL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorUsuario</servlet-name>
        <servlet-class>Controlador.ControladorUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorUsuario</servlet-name>
        <url-pattern>/ControladorUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorURL</servlet-name>
        <url-pattern>/ControladorURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorSuscripcion</servlet-name>
        <url-pattern>/ControladorSuscripcion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPerfil</servlet-name>
        <url-pattern>/ControladorPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorMenu</servlet-name>
        <url-pattern>/ControladorMenu</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
